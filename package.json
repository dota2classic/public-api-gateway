{
  "name": "d2c-api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "GS_URL=62.122.215.162:5003 nest start --watch",
    "start:local": "GS_URL=localhost:5003 nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker": "docker build . -t dota2classic/matchmaking:api-gateway && docker push dota2classic/matchmaking:api-gateway",
    "docker:local": "docker build . -t dota2classic/matchmaking:api-gateway",
    "apigen:gameserver": "openapi-generator  generate --model-name-prefix Gameserver --import-mappings BanReason --import-mappings AchievementKey --import-mappings MatchmakingMode=1 --import-mappings Dota_GameMode=2 --skip-validate-spec -i http://localhost:5003/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated --additional-properties useSingleRequestParameter=false && rm -rf src/generated-api/gameserver && cp -r ./generated src/generated-api/gameserver && rm -rf ./generated && node scripts/after-codegen.js gameserver",
    "apigen:forum": "openapi-generator  generate --model-name-prefix Forum --import-mappings BanReason --import-mappings ThreadType=1 --import-mappings Dota_GameMode=2 --skip-validate-spec -i http://localhost:6009/api-json -g typescript-fetch -t ./.openapi-generator/typescript-fetch-swr -o ./generated --additional-properties useSingleRequestParameter=false && rm -rf src/generated-api/forum && cp -r ./generated src/generated-api/forum && rm -rf ./generated && node scripts/after-codegen.js forum",
    "apigen:tournament": "openapi-generator generate --skip-validate-spec -i http://localhost:6100/api-json -g typescript-axios -o ./generated --config tournament-openapi-generate.json && rm -rf src/generated-api/tournament && cp -r ./generated src/generated-api/tournament && rm -rf ./generated"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.1",
    "@nestjs/core": "^10.4.1",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "@ntegral/nestjs-sentry": "^4.0.1",
    "@willsoto/nestjs-prometheus": "^6.0.1",
    "axios": "^1.7.5",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^2.0.0",
    "cache-manager-redis-yet": "^5.1.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.5",
    "discord.js": "^12.4.1",
    "dotenv": "^8.2.0",
    "ioredis": "^5.4.1",
    "node-fetch": "^2.6.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "passport-steam": "^1.0.18",
    "pg": "^8.7.1",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "rxjs-async-map": "^0.3.0-preview.0",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.3.20",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "4.17.12",
    "@types/express-serve-static-core": "^4.17.21",
    "@types/jest": "26.0.10",
    "@types/node": "^22.5.1",
    "@types/passport": "^1.0.16",
    "@types/passport-http": "^0.3.11",
    "@types/passport-steam": "^1.0.6",
    "@types/supertest": "^2.0.8",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "bignumber.js": "^9.0.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "openapi-typescript-codegen": "^0.5.3",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^5.5.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
